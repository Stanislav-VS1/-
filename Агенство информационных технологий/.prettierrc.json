{
  "$schema": "http://json.schemastore.org/prettierrc",
  "tabWidth": 2,
  "printWidth": 100,
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "quoteProps": "consistent",
  "useTabs": false,
  "proseWrap": "always",
  "endOfLine": "auto"
}
